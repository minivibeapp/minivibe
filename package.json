{
  "name": "minivibe",
  "version": "0.2.30",
  "description": "CLI wrapper for Claude Code with mobile remote control via MiniVibe iOS app",
  "author": "MiniVibe <hello@minivibeapp.com>",
  "homepage": "https://github.com/minivibeapp/minivibe",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minivibeapp/minivibe.git"
  },
  "bugs": {
    "url": "https://github.com/minivibeapp/minivibe/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "vibe": "./dist/cli.js",
    "vibe-agent": "./dist/agent/cli.js"
  },
  "files": [
    "dist/",
    "e2e.js",
    "pty-wrapper.py",
    "pty-wrapper-node.js",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node vibe.js",
    "agent": "node agent/agent.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "dev": "npx tsx src/cli.ts"
  },
  "dependencies": {
    "uuid": "^9.0.0",
    "ws": "^8.14.2"
  },
  "optionalDependencies": {
    "node-pty": "^1.0.0"
  },
  "keywords": [
    "claude",
    "claude-code",
    "cli",
    "remote",
    "mobile",
    "ai",
    "terminal",
    "minivibe",
    "ios"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.17"
  }
}
